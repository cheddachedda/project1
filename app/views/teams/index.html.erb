<h1>Teams#index</h1>
<p>Find me in app/views/teams/index.html.erb</p>

<table>

  <tr>
    <% Team.column_names.each do |col| %>
      <th><%= col %></th>
    <% end %>
  </tr>

  <% @teams.each do |team| %>
    <tr>
      <% Team.column_names.each do |col| %>
        <td><%= team[col] %></td>
      <% end %>
    </tr>
  <% end %>

</table>
